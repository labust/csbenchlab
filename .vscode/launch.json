    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Main",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "CSB_M_PATH": "/home/luka/matlab/csbenchlab",
                "CSB_LIB_PATH": "/home/luka/matlab/csbenchlab/registry"
            },
            "program": "${workspaceFolder}/csbenchlab.py",
            "console": "integratedTerminal",
            "args": [
                "-backend",
                "matlab", //"--daemon-restart",
                "--debug"
            ],
            "justMyCode": false
        },
        {
